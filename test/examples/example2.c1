/* for debugging
   #include <stdio.h> */
const int N = 10;
int arr[10] = {1,5,3,2,10,9,8,6,7,4};
int i = 0, j = 0, t, flag;
int main()
{
    i = 1;
    while (i < N) {
        t = arr[i];
        j = i;
        flag = 1;
        while (flag != 0) {
            if (j == 0)
                flag = 0;
            else if (arr[j - 1] > t) {
                arr[j] = arr[j - 1];
                j = j - 1;
            } else
                flag = 0;
        }
        arr[j] = t;
        i = i + 1;
    }

    i = 0;
    while (i < N) {
        print(arr[i]);
        i = i + 1;
    }
    /* for debugging 
    for (i = 0; i < N; i++)
    printf("%d\n", arr[i]); */
}
