const int N = 10;
int i = 0, j = 0, t, arr[N] = {1,5,3,2,10,9,8,6,7,4};
int main()
{
    i = 1;
    while (i < N) {
        t = arr[i];
        j = i;
        while (j > 0 && arr[j - 1] > t) {
            arr[j] = arr[j - 1];
            j = j - 1;
        }
        arr[j] = t;
        i = i + 1;
    }
}
